# Bio-MCP Configuration
# Copy this file to .env and fill in your API keys

# Required for RAG functionality
OPENAI_API_KEY=your-openai-api-key-here

# Optional: Enhanced PubMed features
PUBMED_API_KEY=your-ncbi-api-key-here

# Optional: Custom service URLs
WEAVIATE_URL=http://localhost:8080
BIO_MCP_DATABASE_URL=sqlite+aiosqlite:///:memory:

# Optional: Server configuration
BIO_MCP_SERVER_NAME=bio-mcp
BIO_MCP_LOG_LEVEL=INFO